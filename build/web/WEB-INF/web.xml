<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LServlet</servlet-name>
        <servlet-class>LServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignServlet</servlet-name>
        <servlet-class>SignServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RServlet</servlet-name>
        <servlet-class>RServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newservet</servlet-name>
        <servlet-class>newservet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VoteMe</servlet-name>
        <servlet-class>VoteMe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>Success.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Success</servlet-name>
        <servlet-class>Success</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LServlet</servlet-name>
        <url-pattern>/LServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignServlet</servlet-name>
        <url-pattern>/SignServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RServlet</servlet-name>
        <url-pattern>/RServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newservet</servlet-name>
        <url-pattern>/newservet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VoteMe</servlet-name>
        <url-pattern>/VoteMe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Success</servlet-name>
        <url-pattern>/Success</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
